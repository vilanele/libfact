print("K = nfinit(polcyclo(5));");
K = nfinit(polcyclo(5));
print("pr = idealprimedec(K,3)[1];");
pr = idealprimedec(K,3)[1];
print("S = [1,x,x^3 + x + 1,[1,2,3,4]~,5,6,x^4 -6, x^3 - 5, 2*x-7];");
S = [1,x,x^3 + x + 1,[1,2,3,4]~,5,6,x^4 -6, x^3 - 5, 2*x-7];
print("h=10;");
h=10;
print("opord(K,pr,S,h,1,#S);");
opord(K,pr,S,h,1,#S);
print("opord(K,pr,S,h,3,#S,&ex,&inv)");
opord(K,pr,S,h,3,#S,&ex,&inv)
print("ex");
ex
print("inv");
inv
