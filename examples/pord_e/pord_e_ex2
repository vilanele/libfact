print("K = nfinit(x^21 + 2);");
K = nfinit(x^21 + 2);
print("pr = idealprimedec(K,5)[1];");
pr = idealprimedec(K,5)[1];
print("S = [x^19 + x - 1,x^3 - 6,3,4,8,9,x,1+x,x^2,x^3,x^3 - 2*x];");
S = [x^19 + x - 1,x^3 - 6,3,4,8,9,x,1+x,x^2,x^3,x^3 - 2*x];
print("pord_e(K,pr,S,6)");
pord_e(K,pr,S,6)
print("pord_e(K,pr,S,#S)");
pord_e(K,pr,S,#S)
